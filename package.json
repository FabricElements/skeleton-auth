    "firebase": "^5.0.4",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@fabricelements/firebase-config": "^0.0.1",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@polymer/iron-image": "^3.0.0-pre.18",
