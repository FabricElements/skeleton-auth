    "url": "git@github.com:FabricElements/skeleton-auth.git"
  },
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
